{
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "browser": true,
        "node": true,
        "jquery": true
    },
    "rules": {
        "quotes": 0,
        "no-trailing-spaces": 1,
        "no-unused-vars": 0,
        "no-underscore-dangle": 1,
        "no-alert": 1,
        "no-lone-blocks": 1
    },
    "globals": {
        "jquery": true,
        "$": true,
        "fabric": true,
        "office-ui-fabric": true
    }
}